{
  "name": "georgia-state-roleplay",
  "version": "1.0.0",
  "description": "Georgia State Roleplay Dashboard and Discord Bot",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:bot": "node src/bot.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:bot\"",
    "dev": "nodemon server.js",
    "dev:bot": "nodemon src/bot.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:bot\"",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "node-fetch": "^2.6.9",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
